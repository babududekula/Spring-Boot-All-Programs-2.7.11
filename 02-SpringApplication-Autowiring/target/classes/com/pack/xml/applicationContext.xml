<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="car" class="com.pack1.Car"  autowire="byType"/>
   
   <bean id="dng1" class="com.pack1.DieselEngine"/>
   
   <bean id="dng" class="com.pack1.DieselEngine" autowire-candidate="false"/> 
   
   <bean id="bike" class="com.pack2.Bike" autowire="byType"/>
   
   <bean id="eng1" class="com.pack2.BikeEngine"/>
   
   <bean id="eng" class="com.pack2.BikeEngine1"/>
   
   

</beans>